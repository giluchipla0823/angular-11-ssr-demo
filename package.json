{
  "name": "angular-11-ssr-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-11-ssr-demo:serve-ssr",
    "serve:ssr": "node dist/angular-11-ssr-demo/server/main.js",
    "build:ssr:full": "npm run hash:i18n && npm run build:ssr && npm run build:ssr:es && npm run build:ssr:en",
    "build:ssr": "npm run hash:i18n && ng build --prod && ng run angular-11-ssr-demo:server:production",
    "build:ssr:es": "ng build --prod --output-path=dist/angular-11-ssr-demo/browser/es --base-href /es/ && ng run angular-11-ssr-demo:server:production",
    "build:ssr:en": "ng build --prod --output-path=dist/angular-11-ssr-demo/browser/en --base-href /en/ && ng run angular-11-ssr-demo:server:production",
    "prerender": "ng run angular-11-ssr-demo:prerender",
    "hash:i18n": "node scripts/hash-translations.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/platform-server": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@nguniversal/express-engine": "^11.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.0.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.15.2",
    "jquery": "^3.4.1",
    "ng-lazyload-image": "^9.1.0",
    "ngx-translate-cache": "^9.0.2",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "md5": "^2.3.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "browser": {
    "path": false,
    "fs": false
  }
}
